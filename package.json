{
  "name": "UFABC-Next-Extension",
  "private": true,
  "version": "1.1.7",
  "description": "Adiciona funcionalidades novas ao sistema de matricula da UFABC",
  "scripts": {
    "dev": "rimraf extension/dev && run-p dev:*",
    "dev:prepare": "node --no-warnings=ExperimentalWarning tasks/prepare.dev.mjs dev",
    "dev:web": "vite",
    "dev:js": "yarn run build:dev --watch src",
    "build": "NODE_ENV=prod run-s clear build:web build:prepare build:prod",
    "build:web": "vite build",
    "build:dev": "tsup src/scripts/background.js src/scripts/contentscript.js --target=node20 --format iife --out-dir extension/dev/dist --no-splitting --onSuccess 'node tasks/mvsw.dev.mjs'",
    "build:prod": "tsup src/scripts/background.js src/scripts/contentscript.js --target=node20 --minify --format iife --out-dir extension/prod/dist --no-splitting --onSuccess 'node tasks/mvsw.prod.mjs'",
    "clear": "rimraf extension/prod"
  },
  "standard": {
    "globals": [
      "chrome"
    ]
  },
  "devDependencies": {
    "@vitejs/plugin-vue2": "^2.3.1",
    "babel-cli": "6.x.x",
    "babel-core": "6.x.x",
    "babel-loader": "7.x.x",
    "babel-preset-env": "1.x.x",
    "chai": "4.x.x",
    "chokidar": "^3.6.0",
    "chromereload": "0.x.x",
    "debounce": "1.x.x",
    "del": "3.x.x",
    "fs-extra": "^11.2.0",
    "gulp": "4.x.x",
    "gulp-bump": "2.x.x",
    "gulp-cache": "0.x.x",
    "gulp-clean-css": "^3.x.x",
    "gulp-filter": "^5.x.x",
    "gulp-git": "^2.x.x",
    "gulp-if": "2.x.x",
    "gulp-imagemin": "4.x.x",
    "gulp-json-transform": "0.x.x",
    "gulp-less": "3.x.x",
    "gulp-livereload": "3.x.x",
    "gulp-plumber": "1.x.x",
    "gulp-sass": "5.1.0",
    "gulp-sequence": "1.x.x",
    "gulp-sourcemaps": "^2.x.x",
    "gulp-tag-version": "1.x.x",
    "gulp-util": "3.x.x",
    "gulp-zip": "^4.x.x",
    "kolorist": "^1.8.0",
    "npm-run-all": "^4.1.5",
    "require-dir": "0.x.x",
    "rimraf": "^5.0.5",
    "sass": "^1.59.3",
    "standard": "^10.0.2",
    "tsup": "^8.0.1",
    "tsx": "^4.7.0",
    "vinyl-named": "1.x.x",
    "vite": "^5.1.1",
    "webpack": "3.x.x",
    "webpack-stream": "3.x.x",
    "yargs": "^8.x.x"
  },
  "dependencies": {
    "autoprefixer": "^9.3.1",
    "axios": "^0.17.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "bluebird": "^3.5.1",
    "caprover": "^1.2.0",
    "compression": "^1.7.3",
    "cors": "^2.8.5",
    "css-loader": "^1.0.1",
    "element-ui": "^2.4.11",
    "express": "^4.16.4",
    "extract-text-webpack-plugin": "^3.0.2",
    "gulp-cssimport": "^6.0.1",
    "gulp-rename": "^1.2.2",
    "gulp-vueify": "^0.0.3",
    "highcharts": "^6.2.0",
    "is_js": "^0.9.0",
    "jquery": "^3.2.1",
    "lodash": "^4.17.4",
    "material-design-icons-iconfont": "^3.0.3",
    "mustache": "^3.0.0",
    "postcss-import": "12.0.1",
    "postcss-load-config": "1.0.0",
    "postcss-loader": "1.0.0",
    "postcss-url": "8.0.0",
    "style-loader": "^0.23.1",
    "toastify-js": "^1.9.0",
    "toastr": "^2.1.4",
    "url-loader": "^1.1.2",
    "vue": "2.7.0",
    "vue-loader": "^15.3.0",
    "vue-style-loader": "^4.1.2",
    "vue-template-compiler": "2.5.17",
    "vue2-highcharts": "^1.2.5",
    "vuedraggable": "^2.16.0",
    "vueify": "^9.4.1",
    "vueify-insert-css": "^1.0.0",
    "vuetify": "1.2.6",
    "webui-popover": "^1.2.18"
  }
}
